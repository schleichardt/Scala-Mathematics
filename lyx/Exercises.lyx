\begin_layout Standard
Für Tabelle 
\begin_inset CommandInset ref
LatexCommand ref
reference "tab:Tabelle-für-Interpolationsaufgabe"

\end_inset

 interpoliere man den Wert 
\begin_inset Formula $z=F(6.8,4.6)$
\end_inset

.
\end_layout

\begin_layout Subsection
Gegeben
\end_layout

\begin_layout Standard
\begin_inset Formula $x=6.8$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $y=4.6$
\end_inset


\end_layout

\begin_layout Subsection
Lösung
\end_layout

\begin_layout Standard
\begin_inset Formula $F(x,y)=w_{00}+w_{01}\cdot\eta+w_{10}\cdot\xi+w_{11}\eta\xi$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\Delta x=2$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\Delta y=1$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\xi=\frac{x-x_{0}}{\Delta x}=\frac{6.8-2}{2}=2,4$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\eta=\frac{y-y_{0}}{\Delta y}=\frac{4,6-3}{1}=1,6$
\end_inset


\end_layout

\begin_layout Standard
Der Vorkommaanteil von 
\begin_inset Formula $\xi$
\end_inset

 ist 2, somit ist j=2.
\end_layout

\begin_layout Standard
Der Vorkommaanteil von 
\begin_inset Formula $\eta$
\end_inset

 ist 1, somit ist m=1.
\end_layout

\begin_layout Standard
\begin_inset Formula $w=\begin{pmatrix}w_{00} & w_{01}\\
w_{10} & w_{11}
\end{pmatrix}=\begin{pmatrix}j+1 & -j\\
-1 & 1
\end{pmatrix}\begin{pmatrix}f_{i,m} & f_{i,m+1}\\
f_{j+1,m} & f_{j+1,m+1}
\end{pmatrix}\begin{pmatrix}m+1 & -1\\
-m & 1
\end{pmatrix}$
\end_inset


\end_layout

\begin_layout Standard
Die f-Werte sind die Funktionswerte um 
\begin_inset Formula $x=6.8$
\end_inset

,
\begin_inset Formula $y=4.6$
\end_inset

 herum, die in der Tabelle sind (schräge Schrift).
\end_layout

\begin_layout Standard
\begin_inset Formula $w=\begin{pmatrix}3 & -2\\
-1 & 1
\end{pmatrix}\begin{pmatrix}4 & 6\\
3 & 4
\end{pmatrix}\begin{pmatrix}2 & -1\\
-1 & 1
\end{pmatrix}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $w=\begin{pmatrix}2 & 4\\
0 & -1
\end{pmatrix}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $F(6.8,4.6)=2+4\cdot1,6+0\cdot2,4+(-1)\cdot1,6\cdot2,4$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $F(6.8,4.6)=4,56$
\end_inset


\end_layout

